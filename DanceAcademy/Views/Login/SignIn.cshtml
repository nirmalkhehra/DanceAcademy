
@{
    ViewBag.Title = "SignIn";
}



<!-- contact -->
<section class="contact py-lg-4 py-md-3 py-sm-3 py-3" id="contact">
    <div class="container py-lg-5 py-md-4 py-sm-4 py-3">
      
        <div class="row">
            <div class="col-lg-12 col-md-12 contact-form pb-lg-3 pb-2">
                <form action="SignInCheck" method="POST">

                    <div class=" form-group contact-forms">
                        <input type="text" name="txtName" id="txtName" class="form-control" placeholder="User Name" required="">
                    </div>
                    <div class=" form-group contact-forms">
                        <input type="password" name="Password" id="Password" class="form-control" placeholder="Password" required="">
                    </div>
                    <input type="submit" class="btn sent-butnn btn-lg" value="Sign In"/>
                    
                </form>
            </div>
            
        </div>
    </div>
</section>